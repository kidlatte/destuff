{
  "version": "1.0",
  "defaultProvider": "jsdelivr",
  "defaultDestination": "Client/wwwroot/lib",
  "libraries": [
    {
      "library": "bootstrap@5.2.1",
      "destination": "Client/wwwroot/lib/bootstrap",
      "files": [
        "dist/css/bootstrap.min.css",
        "dist/js/bootstrap.bundle.min.js"
      ]
    },
    {
      "library": "bootstrap-icons@1.9.1",
      "destination": "Client/wwwroot/lib/bootstrap-icons",
      "files": [
        "font/bootstrap-icons.min.css",
        "font/fonts/*"
      ]
    }
  ]
}